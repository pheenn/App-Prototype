<html>   
<head>
  <meta charset="utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <title>Standard Head</title>

  <!-- Fonts: Plus Jakarta Sans (consistent) -->
  <link rel="preconnect" href="https://fonts.googleapis.com"/>
  <link rel="preconnect" crossorigin href="https://fonts.gstatic.com/"/>
  <link as="style"
        href="https://fonts.googleapis.com/css2?display=swap&amp;family=Plus+Jakarta+Sans:wght@400;500;700;800"
        onload="this.rel='stylesheet'"/>
  <!-- Material Symbols (single consistent variant) -->
  <link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200"
        rel="stylesheet"/>

  <!-- Tailwind (single config used everywhere) -->
  <script src="https://cdn.tailwindcss.com?plugins=forms,container-queries"></script>
  <script id="tailwind-config">
    tailwind.config = {
      darkMode: "class",
      theme: {
        extend: {
          colors: {
            primary: "#13a4ec",
            "background-light": "#f6f7f8",
            "background-dark": "#101c22"
          },
          fontFamily: {
            display: ["Plus Jakarta Sans", "sans-serif"]
          },
          borderRadius: {
            DEFAULT: "0.5rem",
            lg: "1rem",
            xl: "1.5rem",
            full: "9999px"
          }
        }
      }
    };
  </script>

  <!-- Standard base styles (apply same material symbol variable settings everywhere) -->
  <style>
    body { min-height: max(884px, 100dvh); }
    /* Material Symbols consistent baseline */
    .material-symbols-outlined {
      font-variation-settings:
        'FILL' 0,
        'wght' 400,
        'GRAD' 0,
        'opsz' 24;
      font-family: 'Material Symbols Outlined';
      speak: none;
      line-height: 1;
      -webkit-font-smoothing: antialiased;
      -moz-osx-font-smoothing: grayscale;
    }
    /* Use this to switch to filled look where needed */
    .material-symbols-outlined.filled {
      font-variation-settings: 'FILL' 1, 'wght' 400, 'GRAD' 0, 'opsz' 24;
    }
  </style>
</head>
<body class="bg-background-light dark:bg-background-dark font-display">
<div class="flex h-screen flex-col justify-between p-6">
<div class="flex flex-col">
<header class="flex justify-end">
<button class="rounded-full bg-primary/10 p-2 text-content-light dark:text-content-dark">
<span class="material-symbols-outlined"> help </span>
</button>
</header>
<main class="mt-8 flex flex-col gap-6">
<h1 class="text-center text-3xl font-bold text-content-light dark:text-content-dark">
            Welcome Back!
          </h1>
<form class="flex flex-col gap-4">
<div class="flex flex-col gap-1.5">
<label class="text-sm font-medium text-subtle-light dark:text-subtle-dark" for="email">
                Email Address
              </label>
<input class="form-input rounded-lg border-none bg-surface-light px-4 py-3 text-content-light ring-1 ring-inset ring-subtle-light/20 focus:ring-2 focus:ring-inset focus:ring-primary dark:bg-surface-dark dark:text-content-dark dark:ring-subtle-dark/20" id="email" placeholder="Enter your email" type="email"/>
</div>
<div class="flex flex-col gap-1.5">
<label class="text-sm font-medium text-subtle-light dark:text-subtle-dark" for="password">
                Password
              </label>
<div class="flex items-center rounded-lg bg-surface-light ring-1 ring-inset ring-subtle-light/20 focus-within:ring-2 focus-within:ring-inset focus-within:ring-primary dark:bg-surface-dark dark:ring-subtle-dark/20">
<input class="form-input flex-1 border-none bg-transparent px-4 py-3 text-content-light focus:ring-0 dark:text-content-dark" id="password" placeholder="Enter your password" type="password"/>
<button class="px-4 text-subtle-light dark:text-subtle-dark" type="button">
<span class="material-symbols-outlined"> visibility_off </span>
</button>
</div>
</div>
<a class="self-end text-sm font-medium text-primary" href="#">
              Forgot Password?
            </a>
</form>
<div class="mt-4 flex flex-col gap-4">
<button class="flex items-center justify-center gap-3 rounded-lg bg-primary/10 py-3 font-bold text-content-light dark:bg-primary/20 dark:text-content-dark">
